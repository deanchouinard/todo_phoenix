<h2>Show task</h2>

<ul>

  <li>
    <strong>User:</strong>
    <%= @task.user_id %>
  </li>

  <li>
    <strong>Date:</strong>
    <%= @task.date %>
  </li>

  <li>
    <strong>Title:</strong>
    <%= @task.title %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @task.description %>
  </li>

</ul>

<%= link "Edit", to: task_path(@conn, :edit, @task) %>
<%= link "Back", to: task_path(@conn, :index) %>
